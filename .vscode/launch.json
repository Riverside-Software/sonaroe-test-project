{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to PASOE",
      "type": "abl",
      "request": "attach",
      "mode": "pasoe",
      "hostname": "127.0.0.1",
      "port": 8891,
      "appName": "pasoe1",
      "password": "",
      "localRoot": "${workspaceFolder}"
    },
    {
      "name": "Attach to AVM",
      "type": "abl",
      "request": "attach",
      "hostname": "127.0.0.1",
      "mode": "legacy",
      "port": 3099,
      "localRoot": "${workspaceFolder}",
      "pause": true
    },
    {
      "name": "Debug current program / Interactive GUI",
      "type": "abl",
      "request": "launch",
      "program": "${file}",
      "cwd": "${workspaceFolder}",
      "graphicalMode": true
    }
  ]
}