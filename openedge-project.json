{
  "name": "Test Project",
  "version": "1.0",
  "charset": "utf-8",
  "oeversion": "12.8",
  "graphicalMode": false,
  "extraParameters": "",
  "buildPath": [
    { "type": "source", "path": "src/procedures", "excludes": "sample/excl/**" },
    { "type": "source", "path": "src/classes" },
    { "type": "propath", "path": "${DLC}/tty/netlib/OpenEdge.Net.pl", "documentation": "doc/netlib.json" },
    { "type": "propath", "path": "${DLC}/tty/OpenEdge.Core.pl", "documentation": "doc/corelib.json" }
  ],
  "buildDirectory": "build",
  "numThreads": 1,
  "dbConnections": [
    { "name": "sports", "connect": "-db db/sp2k -ld sports -RO", "dumpFile": "dump/sports.df", "aliases": [ "foo", "bar"] }
  ],
  "profiles": [
    { 
      "name": "GUI-Mode",
      "inherits": "default",
      "value": {
        "graphicalMode": true
      }
    },
    {
      "name": "DevContainer",
      "inherits": "default",
      "value": {
        "dbConnections": [
          { "name": "sports", "connect": "-db db/sp2kcont -ld sports -RO", "dumpFile": "dump/sports.df", "aliases": [ "foo", "bar"] }
        ]
      }
    }
  ]
}
