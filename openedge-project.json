{
  "name": "Test Project",
  "version": "1.0",
  "charset": "iso8859-15",
  "oeversion": "12.2",
  "graphicalMode": false,
  "extraParameters": "-ttwrdrul 0",
  "buildPath": [
    { "type": "source", "path": "src/procedures", "excludes": "sample/excl/**" },
    { "type": "source", "path": "src/classes" },
    { "type": "propath", "path": "${DLC}/tty/netlib/OpenEdge.Net.pl", "documentation": "doc/netlib.json" },
    { "type": "propath", "path": "${DLC}/tty/OpenEdge.Core.pl", "documentation": "doc/corelib.json" }
  ],
  "buildDirectory": "build",
  "numThreads": 1,
  "dbConnections": [
    { "name": "sp2k", "connect": "-db db/sp2k -RO", "dumpFile": "dump/sp2k.df", "aliases": [ "foo", "bar"] }
  ],
  "profiles": [
    { 
      "name": "GUI-Mode",
      "inherits": "default",
      "value": {
        "graphicalMode": true
      }
    }
  ]
}
